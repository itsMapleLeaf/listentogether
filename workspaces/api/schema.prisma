generator photonjs {
  provider = "photonjs"
}

datasource sqlite {
  url      = "file:data.db"
  provider = "sqlite"
}

model Room {
  id      String  @id @default(uuid())
  ownerId String  @unique
  tracks  Track[]
}

model Track {
  id         String  @id @default(uuid())
  title      String
  youtubeUrl String?
}